{
  "name": "sentinel-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "migrate": "node ./scripts/migrate.js",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "dotenv": "16",
    "next": "14",
    "pg": "8",
    "react": "18",
    "react-dom": "18",
    "zod": "4",
    "uuid": "9",
    "ioredis": "5"
  },
  "devDependencies": {
    "@types/jest": "29",
    "@types/node": "20",
    "@types/react": "19.2.7",
    "jest": "29",
    "ts-jest": "29",
    "node-mocks-http": "1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
